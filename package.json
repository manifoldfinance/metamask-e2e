{
  "name": "metamask-e2e-dapp",
  "version": "0.0.2",
  "scripts": {
    "deploy": "./deploy.sh",
    "lint": "npx eslint . --ext js,json && prettier ./src -c",
    "lint:fix": "npx eslint . --fix --ext js,json && prettier ./src -c --write",
    "start": "npx webpack-dev-server",
    "build": "npx webpack --mode production",
    "test": "npm run-script lint",
    "prepublishOnly": "npm run-script test && npm run-script build",
    "gh-pages": "npx gh-pages -d ."
  },
  "publishConfig": {
    "access": "private"
  },
  "files": [
    "dist/"
  ],
  "author": "SEE CONTRIBUTORS",
  "license": "MIT",
  "devDependencies": {
    "@metamask/eslint-config": "^4.0.0",
    "@metamask/onboarding": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eth-sig-util": "^2.5.3",
    "ethereumjs-util": "^5.1.1",
    "ethers": "^5.0.18",
    "gh-pages": "2.0",
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
